<!DOCTYPE html>
<html>
<head>
  <title>Dashboard Nilai Calon Mahasiswa</title>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
</head>
<body class="container mt-4">
  <h2>Dashboard Nilai Calon Mahasiswa</h2>
  <ul>
    <li><a href="/">Mulai Ujian</a></li>
    <li><a href="/Dashboard">Dashboard </a></li>
  </ul>
  <table class="table table-bordered mt-3">
    <thead class="table-light">
      <tr>
        <th>Nama</th>
        <th>Rata-rata</th>
        <th>Maksimum</th>
        <th>Minimum</th>
        <th>Jumlah Tes</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(item => { %>
        <tr>
          <td><%= item.nama %></td>
          <td><%= item.rataNilai.toFixed(2) %></td>
          <td><%= item.maxNilai %></td>
          <td><%= item.minNilai %></td>
          <td><%= item.jumlahTes %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
